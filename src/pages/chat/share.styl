.flup-content
  width 100%
  height 100%
  .flup-content-color
    height 20px
    width 100%
    background-color #f3f3f3
  .share-tit
    font-size 32px
    text-align center
    height 100px
    line-height 100px
    overflow:hidden; 
    text-overflow:ellipsis;
    display:-webkit-box; 
    -webkit-box-orient:vertical;
    -webkit-line-clamp:1; 
    word-wrap:break-word; 
    word-break:normal;
    span
      color #36bac9
  .share-write
    height 80px
    line-height 80px
    text-align center
    font-size 32px
    background-color #fb699d
    color #fff
    position fixed
    width 100%
    z-index 100
    i
      display inline-block
      background url('/build/img/toast-share.png') no-repeat top center
      background-size 100% 100%
      width 30px
      height 30px
      margin-right 20px
  .flup-info
    width 90%
    margin 0 auto
    .flup-info-tit
      font-size 28px
      color #919dae
      padding-top 50px
    .flup-id,.flup-name,.flup-iphone
      font-size 30px
      color #344559
      margin-top 50px
      span
        color #919dae
        width 150px
        display inline-block
        position relative
      span:before
        width 12px
        height 11px
        content ""
        display inline-block
        background url('/build/img/required-flag.png') no-repeat top center
        background-size 100% 100%
        margin-bottom 8px
        margin-right 10px
      input
        border none
        width 520px
        height 50px
        line-height 50px
        border-bottom 0.0275rem solid #dbdbdb
      input[disabled],input:disabled
        color #344559 !important
        -webkit-tap-highlight-color rgba(52,69,89,1) !important
    .flup-name
      margin 40px 0
    .flup-iphone
      padding-bottom 60px
  .flup-table
    width 90%
    margin 0 auto 60px
    .flup-table-tit
      text-align center
      font-size 34px
      color #344559
      margin 60px 0 0
      word-wrap:break-word; 
      word-break:normal;
    .flup-from:first-child
      .from-tit
        margin-top 0
    .flup-from
      .from-tit
        font-size 32px
        color #4e5e75
        line-height 48px
        margin-top 80px
        word-wrap:break-word; 
        word-break:normal;
        position relative
        span
          display inline-block
          width 93%
          margin-left 7%
        i
          position absolute
          top 0
          left 0
      .apply_from_subject
          padding 10px 0
          label
            display block
            color #4e5e75
            font-size 32px
            padding-left 7.5%
            margin-top 50px
            position relative
            input[type=radio]
              width 48px
              height 48px
              border-radius 50%
              color #fff
              border none
              margin-right 25px
              position absolute
              top 1px
            input[type=radio].topi
              top 1px
            input[type=tel]
              background-color #43547d
              color #fff
              border-top none
              border-left none
              border-right none
              border-bottom 3px solid #fff
              height 35px
              line-height 35px
              padding-bottom 10px
            input[type=checkbox]
              width 48px
              height 48px
              background-color #fff
              color #fff
              border none
              margin-right 25px
              position absolute
              top 1px
            input[type=checkbox].topi
              top 1px
            input[type=text]
              background-color #43547d
              color #fff
              border-top none
              border-left none
              border-right none
              border-bottom 3px solid #fff
              height 35px
              line-height 35px
              padding-bottom 10px
            i
              color #919dae
              font-size 30px
              position absolute
              top 1px
              left 7.4%
              font-style normal
              line-height 48px
              width 49px
              height 49px
              background-color #fff
              text-align center
              border 0.0275rem solid #c0c6d0
              z-index 10
            i.Iradio
              border-radius 50%
            i.Icheckbox
              border-radius none
            i.topi
              top 1px
            i.topradio
              top 1px
            span
              margin-left 70px
              font-size 32px
              line-height 48px
              width 88%
              display block
              word-wrap:break-word; 
              word-break:normal;
            span.span_content
              margin-left 0
            span.Multiline
              lin-height 25px
              display inline-block
          label[aria-checked="true"]
            input[type=radio]
              background-color #36bac9
            input[type=checkbox]
              background-color #36bac9
            i
              background-color #36bac9
              border-color #36bac9
              color #fff
          label[aria-checked="false"]
            input[type=radio]
              background-color #fff
            input[type=checkbox]
              background-color #fff
            input[type=text]
              background-color #43547d
            input[type=tel]
              background-color #43547d
            i
              background-color #fff
          label:hover
            input[type=text]
              background-color #36476f
            input[type=tel]
              background-color #36476f
          .inputCheckbox
            display block
            height 250px
            line-height 35px
            font-size 30px
            background-color #fff
            color #fff
            width 560px
            border none
            margin-left 80px
            padding 10px
      ul
        margin-left 50px
        li
          margin-top 50px
          font-size 32px
          color #4e5e75
          i
            display inline-block
            width 48px
            height 48px
            font-size 30px
            color #919dae
            background-color #fff
            border 0.0275rem solid #c0c6d0
            text-align center
            margin-right 30px
          i.check
            background-color #36bac9
            color #fff
            border none
    
    .from-radio
      li
        i
          border-radius 50%
    .from-checkbox
      li
        i
          border-radius 2px
    .from-pack
      .from-pack-con
        margin-top 50px
        margin-left 50px
        font-size 32px
        color #344559
        line-height 48px
        textarea
          padding 30px
          line-height 48px
          color #344559
          width 615px
          height 210px
          font-size 32px
          border 0.0275rem solid #dbdbdb
          outline: none;
          -webkit-appearance: none;
          -webkit-tap-highlight-color: rgba(0,0,0,0)
        .weui_textarea_counter
          text-align right
          font-size 24px
          .num
            color #36bac9
  .flup-data
    width 90%
    margin 0 auto
    .flup-data-tit
      font-size 28px
      color #919dae
      margin-top 43px
    .flup-data-link
      color #36bac9
      font-size 32px
      margin-top 35px
      margin-bottom 65px
      a
        color #36bac9
        text-decoration none
        overflow:hidden; 
        text-overflow:ellipsis;
        display:-webkit-box; 
        -webkit-box-orient:vertical;
        -webkit-line-clamp:1; 
        word-wrap:break-word; 
        word-break:normal;
  .flup-btn
    height 100px
    line-height 100px
    background-color #36bac9
    text-align center
    color #fff
    font-size 30px
    border none
    width 100%
    display block
    text-decoration none

.flup-eject
  background-color rgba(0,0,0,0.5)
  width 100%
  height 100%
  position fixed
  top 0
  left 0
  z-index 120
  display none
  .eject-con
    margin 25% auto
    background-color #fff
    text-align center
    border-radius 10px
    width 580px
    p
      .cancel
        font-size 30px
        color #919dae
        height 106px
        line-height 106px
        width 50%
        display inline-block
        border-right 0.0275rem solid #dbdbdb
      .sure
        font-size 30px
        color #36bac9
        height 106px
        line-height 106px
        width 50%
        display inline-block
    .eject-tit
      font-size 32px
      color #4e5e75
      border-bottom 0.0275rem solid #dbdbdb
      padding 80px
      

