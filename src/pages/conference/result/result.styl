.conference-order
  background-color #f3f3f3
  width 100%
  padding-top 20px
  .conference-order-num
    background-color #fff
    padding-left 40px
    // padding-right:  40px
    padding-bottom: 40px
    a
        display block
        .order-num-tit
          height 114px
          line-height 114px
          color #919dae
          font-size 26px
          position relative
          span
            position absolute
            right 40px
        .apply-prod
            width: 100%
            padding: 40px 0;
            border-top: 0.0275rem solid #dbdbdb
            border-bottom: 0.0275rem solid #dbdbdb
            position: relative;
            .apply-prod-img
                width: 220px
                height: 160px
                float: left
                margin-right: 30px
            p
                width: 420px
                min-height: 90px
                margin-bottom: 20px
                float: left
                display -webkit-box
                -webkit-box-orient vertical
                overflow hidden
                text-overflow ellipsis
                -webkit-line-clamp 3
                word-wrap:break-word; 
                word-break:normal;
                color #344559
                font-size 32px
            .apply-prod-num
                float: left
                width: 420px
                height: 50px
                line-height: 50px
                bottom: 0;
                right: 0;
                display: -webkit-box;
                display: -webkit-flex;
                display: -ms-flexbox;
                display: flex;
                -webkit-box-pack: space-between;
                -webkit-justify-content: space-between;
                justify-content: space-between
                .apply-prod-money
                    font-size: 26px
                    color: #919dae
                .apply-prod-num-modify
                    display: inline-block
                    span
                        font-size: 24px
                        color: #919dae
                        padding: 10px 20px
        .apply-prod:after
            content:".";        
            display:block;        
            height:0;        
            clear:both;        
            visibility:hidden; 
        .apply-price
            width: 100%
            text-align: right
            display: inline-block
            padding-top: 40px
            padding-right: 40px
            font-size: 26px
            color: #919dae
            .apply-price-money
                color: #fb699d
            .apply-prod-cont:after
                content:".";        
                display:block;        
                height:0;        
                clear:both;        
                visibility:hidden; 
  .conference-order-patient
    background-color #fff
    margin-top 20px
    padding: 40px;
    p
        padding-bottom: 30px
        .order-title
            color: #919dae;
            font-size 30px
            padding: 30px 0;
        .order-cont
            font-size: 30px;
            line-height: 36px
            color #344559
            word-break: break-all;
    p:last-child
        padding-bottom: 0px
  .conference-order-ask
    background-color #fff
    margin-top 30px
    padding-left 5%
  .conference-order--info
    background-color #fff
    margin-top 20px
    padding-bottom: 40px
    padding-left 5%
    .order-title
        color #919dae
        font-size 32px
        height: 116px;
        line-height: 116px; 
        border-bottom: 0.0275rem solid #dbdbdb
    .orderConts
      color #344559
      font-size 30px
      margin-bottom 30px
      word-break: break-all;
      span
        color #919dae
    .cont1
      margin-top 40px
    .cont3
       margin-bottom 0px 
  .conference-order-bottom
    font-size 26px
    line-height 36px
    color #919dae
    width 90%
    margin: 0 auto;
    padding: 42px 0 90px 0
    p
        padding-bottom: 26px
        span
            float: left;
            display: inline-block
        .bottom-cont
            float: inherit
            width: 93%
    p:first-child
        padding-bottom: 40px
    a
      color #36bac9