.opera-box
  width 100%
  height auto
  background-color #f3f3f3
  padding-top 20px
  .opera-item-order
    background-color #fff
    padding 0px 30px
    .opera-num-tit
      height 115px
      line-height 115px
      font-size 24px
      color #919dae
      position relative
      span
        position absolute
        right 0
    .opera-num-main
      width 100%
      padding-bottom 50px
      position relative
      .opera-num-left
        width 220px
        height 160px
        overflow hidden
        display inline-block
      .opera-num-right
        display inline-block
        position absolute
        right 0
        top 0
        height 160px
        width 100%
        padding-left 250px
        .tit
          font-size 32px
          color #344559
        .opera-state
          position absolute
          right 0
          top 0
          font-size 28px
          color #fb699d
        .state-active1
          color #36bac9
        .state-active2
          color #c0c6d0
        .opera-see
          position absolute
          right 0
          bottom 0
          font-size 28px
          color #919dae
          padding 8px 30px 10px
          border 0.0275rem solid #c0c6d0
          border-radius 32px
          text-align center
        .opera-read
          position absolute
          left 250px
          bottom 0
          font-size 28px
          color #919dae
          padding 8px 30px 10px
          border 0.0275rem solid #c0c6d0
          border-radius 32px
          text-align center
      .order-close
        color #fb699d
        font-size 26px
        position relative
        word-wrap:break-word; 
        word-break:normal;
        margin-top 30px
        word-wrap:break-word;
        i
          width 30px
          height 30px
          display inline-block
          background:url("http://patienth5.szyyky.com/build/imgWX/icon8.png") no-repeat top center
          background-size cover
          margin-right 15px
          margin-top 7px
  .opera-expert
    background-color #fff
    margin-top 20px
    .opera-expert-empty
      font-size 30px
      color #c0c6d0
      text-align center
      padding 105px 0px;
    .opera-expert-main
        display block
        padding 50px 30px;
        .opera-expert-tit
            font-size 28px
            color #919dae
            margin-bottom 40px
        ul
          li
            margin-bottom 45px
            position relative
            height 140px
            .opera-expert-name
                position absolute
                img
                  width 140px
                  height 140px
                  border-radius 70px
            .opera-expert-info
                width 75%
                position relative
                margin-left 25%
                .name
                  font-size 32px
                  color #344559
                .info
                  font-size 26px
                  color #919dae
                  margin 12px 0px 15px
                .shanchang
                  font-size 28px
                  color #4e5e75
                  text-overflow:ellipsis; 
                  white-space:nowrap; 
                  overflow:hidden;
          li:last-child
            margin-bottom 0px
  .opera-item-sit
    background-color #fff
    margin-top 20px
    padding 0 30px 50px
    .opera-item-headline
      font-size 32px
      color #344559
      padding-top 45px
    .item-headline
      font-size 28px
      color #919dae
      position relative
      margin-top 36px
      i
        width 38px
        height 38px
        display inline-block
        margin-right 25px
        position relative
        top 7px
      i.man
        background:url("http://patienth5.szyyky.com/build/imgWX/man.png") no-repeat top center
        background-size cover
      i.woman
        background:url("http://patienth5.szyyky.com/build/imgWX/woman.png") no-repeat top center
        background-size cover
    .opera-tit
      height 100px
      line-height 100px
      color #919dae
      font-size 28px
    .opera-time
      font-size 30px
      color #4e5e75
      word-wrap:break-word;
    .time1
      margin-bottom 20px
    div.opera-img
        overflow hidden
        ul
          li
            display inline-block
            position relative
            width 210px
            height 210px
            margin-right 30px
            margin-bottom 30px
            overflow hidden
            float left
            img
              min-width 100%
              min-height 100%
          li:nth-child(3n)
            margin-right 0
  .opera-pay
    background-color #fff
    margin-top 20px
    padding 54px 30px 58px
    .opera-pay-details1
        .opera-tit
          font-size 30px
          color #919dae
          position relative
          .opera-invoice
            position absolute
            right 0
            color #36bac9
            font-size 28px
        ul
          li
            border 1px solid #dbdbdb /*no*/
            position relative
            height 100px
            line-height 100px
            margin-top 25px
            font-size 28px
            color #919dae
            padding 0 35px
            .opera-pay-tit
              font-size 30px
              color #344559
            .opera-money
              position absolute
              left 465px
              .opera-pay-money
                font-size 34px
                color #fb699d
    .opera-pay-details2
        .opera-tit
          font-size 30px
          color #919dae
          position relative
        ul
          li
            border 1px solid #dbdbdb /*no*/
            position relative
            height 100px
            line-height 100px
            font-size 28px
            color #919dae
            padding 0 35px
            margin-top 25px
            .opera-pay-tit
              font-size 30px
              color #344559
            .fuwub
              position absolute
              right 35px
              .opera-pay-num
                color #fb699d
  .opera-item-bottom
    background-color #f3f3f3
    text-align center
    padding 52px 0 150px
    p
      color #919dae
      font-size 26px
      a
        color #36bac9
  .opera-payment
    height 100px
    line-height 100px
    position fixed
    bottom -2px
    width 100%
    z-index 99
    .opera-payment-count
      display inline-block
      width 60%;
      background-color #344559
      color #c0c6d0
      font-size 28px
      padding-left 34px
      span
        color #fb699d
    .opera-payment-btn
        display inline-block
        width 40%
        background-color #36bac9
        font-size 30px
        color #fff
        text-align center
        position absolute
        right 0;
.opera-defrayal
    position fixed
    z-index 100
    top 0;
    left 0;
    width 100%
    height 100%
    background-color rgba(0,0,0,0.2)
    display none
    .opera-defrayal-main
        background-color #fff
        position absolute
        bottom 0
        width 100%
        .opera-defrayal-tit
            position relative
            text-align center
            font-size 30px
            color #4e5e75
            margin 60px auto 75px
            i.opera-defrayal-close
                width 30px
                height 30px
                display block
                position absolute
                background:url("http://patienth5.szyyky.com/build/imgWX/close.png")no-repeat top center
                background-size 100% 100%
                right 33px
                top 10px
            i.opera-defrayal-return
                width 38px
                height 32px
                display none
                position absolute
                background:url("http://patienth5.szyyky.com/build/imgWX/return.png")no-repeat top center
                background-size 100% 100%
                left 30px
                top 10px
        .opera-defrayal-name
            font-size 34px
            color #344559
            padding 0 30px
            position relative
            margin-bottom 60px
            .opera-pay-money
                color #919dae
                font-size 30px
                position absolute
                right 30px
                i
                  font-size 34px
                  color #fb699d
        .opera-defrayal-pay
            text-align center
            -webkit-tap-highlight-color: rgba(0, 0, 0, 0);
            -webkit-appearance:none;
            ul
              -webkit-tap-highlight-color: rgba(0, 0, 0, 0);
              -webkit-appearance:none;
              li
                border 1px solid #dbdbdb
                position relative
                text-align center
                width 74%
                margin 0 auto
                height 100px
                line-height 100px
                margin-top 25px
                -webkit-tap-highlight-color: rgba(0, 0, 0, 0);
                -webkit-appearance:none;
                .pay-wx
                  width 190px
                  height 53px
                .pay-zfb
                  width 176px
                  height 50px
                .pay-ewm
                  width 218px
                  height 50px
              li.payActive
                border-color #36bac9
                i
                  width 50px
                  height 50px
                  position absolute
                  background:url("http://patienth5.szyyky.com/build/imgWX/icon9.png")no-repeat top center
                  background-size 100% 100%
                  right 0
                  bottom 0
            .opera-defrayal-agree
                font-size 26px
                margin-bottom 40px
                margin-top 34px
                text-align center
                a
                  color #36bac9
        .opera-pay-ewm
          display none
          position relative
          img
            display block
            width 54.5%
            margin 0 auto 50px;
      .opera-pay-confirm
        display none
        width 100%
        text-align center
        .layout-main
          background-color #fff
          width 77.5%
          position absolute
          left 11.25%
          top 35%
          text-align center
          border-radius 10px
          p
            color #4e5e75
            font-size 32px
            border-bottom 1px solid #dbdbdb /*no*/
            margin-top 92px
            padding-bottom 62px
          a
            font-size 30px
            color #919dae
            height 100px
            line-height 100px
            display inline-block
            width 50%
          a.quit
            border-right 1px solid #ddd /*no*/
          a.cancel
            color #36bac9
      .opera-pay-loading
        width 100%
        position absolute
        margin 0 auto;
        top 40%
        display none
        .layout-main
          width 25%
          background-color rgba(0, 0, 0, .75)
          border-radius 8%
          margin 0 auto
          text-align center
          img
            width 40%
            margin 20% auto
          
          
                
        