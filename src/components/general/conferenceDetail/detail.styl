.conference-detail-box
    width: 100%
    height: 100%
    background-color: #f3f3f3
    .detail-main
        width: 100%
        height: 100%
        background-color: #f3f3f3
        .detail-head
            width: 100%
            background-color: #ffffff
            margin-bottom: 20px
            .detail-adimg
                position: relative
                .item-img
                    width: 100%
                    height: 380px
                    overflow: hidden
                    img
                        width: 100%
                        max-height: 100%
                .detail-adimg-time
                    position: absolute
                    bottom: 0;
                    width: 100%;
                    height: 100px
                    line-height: 100px
                    text-align: center
                    color: #ffffff
                    font-size: 30px
                    background-color: rgba(34,40,50,0.4)
                    display: block
            .detail-desc
                width: 100%
                padding: 42px 20px
                padding-bottom: 0px
                margin-bottom: 40px
                font-size: 30px
                line-height: 40px
                color: #344559
                display -webkit-box
                -webkit-box-orient vertical
                overflow hidden
                text-overflow ellipsis
                -webkit-line-clamp 2
                word-wrap:break-word; 
                word-break:normal;
            .detail-time
                width: 100%
                padding: 0 20px
                font-size: 26px
                color: #919dae
                img
                    width:34px;
                    height: 34px
                    margin-right: 20px
                    margin-bottom: 8px
            .detail-address
                width: 100%
                padding: 0 20px
                margin: 23px 0 40px
                font-size: 26px
                color: #919dae
                overflow:hidden; 
                text-overflow:ellipsis;
                display:-webkit-inline-box; 
                -webkit-box-orient:vertical;
                -webkit-line-clamp:1; 
                word-wrap:break-word; 
                word-break:normal;
                img
                    width:36px;
                    height: 36px
                    margin-right: 20px
                    margin-bottom: 8px
        .detail-cont
            width: 100%
            padding: 10px 40px
            // padding-bottom: 160px
            background-color: #ffffff
            .detail-tab
                width: 100%
                margin-bottom: 10px
                .detail-tab-container
                    width: 100%
                    display: -webkit-box;
                    display: -webkit-flex;
                    display: -ms-flexbox;
                    display: flex;
                    -webkit-box-pack: space-between;
                    -webkit-justify-content: space-between;
                    justify-content: space-between
                    position: relative
                    .detail-tab-item
                        float: left
                        position relative
                        a
                            padding: 30px 22px;
                            margin:0 28px;
                            font-size: 30px
                            color: #344559
                            display: inline-block
                            // border-bottom: 5px solid #ffffff
                            -webkit-transition: color .3s cubic-bezier(.645,.045,.355,1);
                            transition: color .3s cubic-bezier(.645,.045,.355,1);
                        a:hover
                            color: #36bac9
                            border-bottom-color:#36bac9
                        .tab-active
                            color: #36bac9
                            border-bottom-color:#36bac9
                        .tab-bar
                            display: block;
                            transform: translate3d(0px, 0px, 0px);
                            width: 80px;
                            margin-left:-40px;
                            left 50%
                            z-index: 1;
                            position: absolute;
                            bottom: 1px;
                            box-sizing: border-box;
                            height: 5px;
                            display none
                            background-color: #36bac9;
                            -webkit-transform-origin: 0 0;
                            -ms-transform-origin: 0 0;
                            transform-origin: 0 0;
                            -webkit-transition: width .3s cubic-bezier(.645,.045,.355,1),-webkit-transform .3s cubic-bezier(.645,.045,.355,1);
                            transition: width .3s cubic-bezier(.645,.045,.355,1),-webkit-transform .3s cubic-bezier(.645,.045,.355,1); 
                            transition: transform .3s cubic-bezier(.645,.045,.355,1),width .3s cubic-bezier(.645,.045,.355,1); 
                            transition: transform .3s cubic-bezier(.645,.045,.355,1),width .3s cubic-bezier(.645,.045,.355,1),-webkit-transform .3s cubic-bezier(.645,.045,.355,1);
                    .detail-tab-item:first-child
                        .tab-bar
                            display block
            .detail-tab-cont
                width: 100%
                overflow: hidden;
                padding-bottom: 160px
                min-height: 500px
                .tab-container
                    width: 2010px
                    display: -webkit-box;
                    display: -webkit-flex;
                    display: -ms-flexbox;
                    display: flex;
                    padding-top 20px
                    .detail-tab-introduce
                        display: block
                        width: 670px
                        .introduce-headimg
                            width: 100%
                        .introduce-desc
                            font-size: 30px 
                            line-height: 36px
                            color: #344559
                            padding: 30px 0;
                            padding-bottom: 27px;
                    .detail-tab-schedule
                        width: 670px
                        display: block
                    .detail-tab-guest
                        width: 670px
                        display: block
                    .show
                        display: block
        .detail-submit
            width: 100%
            height: 100px
            line-height: 100px;
            position: fixed
            bottom: 0;
            font-size: 30px
            color: #ffffff
            text-align: center
            span
                font-size: 30px
                pointer-events: none
        .disable-submit
            background-color: #919dae
            cursor: none
        .able-submit
            background-color: #2bb0bd

            
            


